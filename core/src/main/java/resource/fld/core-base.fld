	<!-- listEach -->
	<function>
		<name>listEach</name>
		<class>lucee.runtime.functions.list.ListEach</class>
		<member-name>listEach,each</member-name>
		<keywords>list,string,iterator</keywords>
		<description>call the given UDF/Closure with every value in the string list.</description>
		<argument>
			<name>list</name>
			<type>string</type>
			<required>Yes</required>
			<description>string list to iterate</description>
		</argument>
		<argument>
			<name>closure</name>
			<alias>function,callback,udf</alias>
			<type>function</type>
			<required>Yes</required>
			<description>function/closure that implements the following constructor [function(any value, numeric index,string list, string delimiter):void].</description>
		
		</argument>
		<argument>
			<name>delimiter</name>
			<default>,</default>
			<type>string</type>
			<required>No</required>
			<description>delimiter used to separate the string list</description>
		</argument>
		<argument>
			<name>includeEmptyFields</name>
			<default>false</default>
			<type>boolean</type>
			<required>No</required>
			<description>include empty fields or not</description>
		</argument>
