        <argument>
            <name>iterations</name>
            <type>number</type>
            <required>no</required>
            <description>number of Iterations for Password Based Encryption algorithms (ignored for all other algorithms). NIST recommends a minimum value of 1000.</description>
        </argument>
		<return>
			<type>string</type>
		</return>
	</function>
