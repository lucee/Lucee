	<!-- arrayEach -->
	<function>
		<name>arrayEach</name>
		<class>lucee.runtime.functions.arrays.ArrayEach</class>
		<member-name>each</member-name>
		<member-chaining>true</member-chaining>
		<keywords>array,iterator</keywords>
