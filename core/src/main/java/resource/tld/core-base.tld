		<attribute>
			<type>any</type>
			<name>endTime</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>The time when the scheduled task ends. Enter a value in seconds.</description>
		</attribute>
		<attribute>
			<type>string</type>
			<name>interval</name>
			<required>false</required>
