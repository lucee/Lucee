		</attribute>
		<attribute>
			<type>string</type>
			<name>folder</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description></description>
		</attribute>
		<attribute>
			<type>string</type>
			<name>newfolder</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description></description>
		</attribute>
		<attribute>
			<type>boolean</type>
			<name>recurse</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description></description>
