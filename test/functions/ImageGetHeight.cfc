component extends="org.lucee.cfml.test.LuceeTestCase"	{
	variables.base64="";
	
	//public function beforeTests(){}
	
	//public function afterTests(){}
	
	public function setUp() localmode="true"{
		variables.binary=toBinary(variables.base64);
		variables.image=imagenew(variables.binary);
	}

	public void function testImage() localmode="true"{
		assertEquals(59,ImageGetHeight(image));
		assertEquals(59,image.getHeight());

	}

	public void function testVariableName() localmode="true"{
		assertEquals(59,ImageGetHeight("variables.image"));
	}

	public void function testBinary() localmode="true"{
		assertEquals(59,ImageGetHeight(variables.binary));
	}

	public void function testBase64() localmode="true"{
		assertEquals(59,ImageGetHeight("data:image/png;base64,"&variables.base64));
	}
} 
