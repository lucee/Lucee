	function testQueryEachCallback(){

		/* 
			as this a threading test, it may or may not fail on the github action runners,
			as they are smaller machines, with not many cores, compare to Lucee developer laptops!
		*/

		copyTableFor( threads = 10 ); // works
		copyTableForIn( threads = 1 ); // works
		copyTableForIn( threads = 10 ); // fails

		for ( var e in variables.errors ){
			systemOutput( e.message, true );
			systemOutput( e.error.message, true );
			systemOutput( e.error.stacktrace, true );
			systemOutput( " ", true );
		}

		expect( variables.errors ).toBeEmpty();
	}


	private function copyTableForIn( required numeric threads ) {
		var qColumns = duplicate( qColumnsCopy );
		var qData = duplicate( qDataCopy );

		queryEach( qData, function( row ) {
			var testcompare = {};
			for( var col in qColumns ) {
				try {
					structinsert( testcompare, col.COLUMN_NAME, "test" );
				} catch( any cfcatch ) {
					trackError( cfcatch, "copyTableForIn: FOR-IN-#threads# ERROR: #col.COLUMN_NAME# collision" );
				}
			}
		}, ( arguments.threads > 1 ), arguments.threads );
	}


	private function copyTableFor( required numeric threads ) {
		var qColumns = duplicate( qColumnsCopy );
		var qData = duplicate( qDataCopy );

		queryEach( qData, function( row ) {
			var testcompare = {};
			for( var x = 1; x <= qColumns.recordCount; x += 1 ) {
				var col = queryGetRow( qColumns, x );
				try {
					structinsert( testcompare, col.COLUMN_NAME, "test" );
				} catch( any cfcatch ) {
					trackError( cfcatch, "copyTableFor: FOR-#threads# ERROR: #col.COLUMN_NAME# collision" );
				}
			}
		}, ( arguments.threads > 1 ), arguments.threads );
	}

	private function trackError( error, message ){
		arrayAppend(variables.errors, {
			message: message, 
			error: error 
		} );
	}
